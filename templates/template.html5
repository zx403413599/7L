<!DOCTYPE html>
<html lang="zh-CN">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap -->
    <link href="https://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <style>
        @media (min-width: 1200px) {
          .container{
            max-width: 970px;
          }
        }
    </style>
$for(author-meta)$
  <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$">
$endif$
  <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
  <style type="text/css">code{white-space: pre;}</style>
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
$if(quotes)$
  <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
  <style type="text/css">
$highlighting-css$
  </style>
$endif$
$for(css)$
  <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
  $math$
$endif$
$for(header-includes)$
  $header-includes$
$endfor$
</head>
<body>
<div class="container">
$for(include-before)$
$include-before$
$endfor$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$
$for(author)$
<h2 class="author">$author$</h2>
$endfor$
$if(date)$
<h3 class="date">$date$</h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
</div>
</body>
<script>
  l = document.getElementsByTagName("a");
  for(var i = 0; i < l.length; ++i) {
    var a = l[i];
    var parent = a.parentElement
    parent.removeChild(a);
    var head = document.createElement("span");
    head.textContent = "[" + a.text + "] ";
    head.className = "mdlink";
    parent.appendChild(head);
    var left = document.createElement("span");
    left.textContent = "(";
    parent.appendChild(left);
    a.text = a.getAttribute("href");
    parent.appendChild(a);
    var right = document.createElement("span");
    right.textContent = ")";
    parent.appendChild(right);
  }
  l = document.getElementsByTagName("figure")
  for(var i = 0; i < l.length; ++i) {
    var figure = l[i];
    var img = figure.getElementsByTagName("img")[0];
    var figcaption = figure.getElementsByTagName("figcaption")[0];
    figure.removeChild(figcaption);
    var p = document.createElement("p");
    var head = document.createElement("span");
    head.textContent = "![" + figcaption.textContent + "]"
    var left = document.createElement("span")
    left.textContent = "("
    var right = document.createElement("span")
    right.textContent = ")"
    var a = document.createElement("a")
    a.href = img.src
    a.text = img.getAttribute("src")
    p.appendChild(head)
    p.appendChild(left)
    p.appendChild(a)
    p.appendChild(right)
    figure.appendChild(p)
  }
</script>
<script src="https://cdn.bootcss.com/jquery/2.1.4/jquery.min.js"></script>
<script src="https://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</html>
